{
	"baseConfig": {
		"parserOptions": {
			"sourceType": "module"
		},
		"env": {
			"browser": true,
			"es6": true
		},
		"plugins": [
			"security",
			"no-document-cookie"
		],
		"extends": [
			"plugin:security/recommended"
		],
		"rules": {
			"security/detect-non-literal-fs-filename": 0,
			"security/detect-object-injection": 0,
			"no-document-cookie/no-document-cookie": 2
		}
	}
}