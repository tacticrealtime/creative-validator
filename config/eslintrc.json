{
	"baseConfig": {
		"parserOptions": {
			"sourceType": "module"
		},
		"env": {
			"browser": true,
			"es6": true
		},
		"plugins": [
			"scanjs-rules",
			"no-unsafe-innerhtml",
			"security",
			"no-document-cookie",
			"no-storage"
		],
		"extends": [
			"plugin:security/recommended"
		],
		"rules": {
			"scanjs-rules/assign_to_hostname": 1,
			"scanjs-rules/assign_to_href": 1,
			"scanjs-rules/assign_to_location": 1,
			"scanjs-rules/assign_to_onmessage": 1,
			"scanjs-rules/assign_to_pathname": 1,
			"scanjs-rules/assign_to_protocol": 1,
			"scanjs-rules/assign_to_search": 1,
			"scanjs-rules/assign_to_src": 0,
			"scanjs-rules/call_Function": 0,
			"scanjs-rules/call_addEventListener": 0,
			"scanjs-rules/call_addEventListener_cellbroadcastmsgchanged": 0,
			"scanjs-rules/call_addEventListener_deviceproximity": 1,
			"scanjs-rules/call_addEventListener_message": 1,
			"scanjs-rules/call_connect": 1,
			"scanjs-rules/call_eval": 1,
			"scanjs-rules/call_execScript": 1,
			"scanjs-rules/call_generateCRMFRequest": 0,
			"scanjs-rules/call_getDeviceStorage_apps": 0,
			"scanjs-rules/call_getDeviceStorage_crashes": 0,
			"scanjs-rules/call_getDeviceStorage_music": 0,
			"scanjs-rules/call_getDeviceStorage_pictures": 0,
			"scanjs-rules/call_getDeviceStorage_sdcard": 0,
			"scanjs-rules/call_getDeviceStorage_videos": 0,
			"scanjs-rules/call_hide": 0,
			"scanjs-rules/call_open_attention": 0,
			"scanjs-rules/call_open_remote=true": 0,
			"scanjs-rules/call_parseFromString": 0,
			"scanjs-rules/call_setImmediate": 0,
			"scanjs-rules/call_setInterval": 0,
			"scanjs-rules/call_setMessageHandler_connect": 0,
			"scanjs-rules/call_setTimeout": 0,
			"scanjs-rules/call_write": 0,
			"scanjs-rules/call_writeln": 0,
			"scanjs-rules/identifier_indexedDB": 0,
			"scanjs-rules/identifier_localStorage": 2,
			"scanjs-rules/identifier_sessionStorage": 2,
			"scanjs-rules/new_Function": 0,
			"scanjs-rules/new_Notification": 0,
			"scanjs-rules/property_addIdleObserver": 0,
			"scanjs-rules/property_createContextualFragment": 0,
			"scanjs-rules/property_geolocation": 0,
			"scanjs-rules/property_getDataStores": 0,
			"scanjs-rules/property_getDeviceStorage": 0,
			"scanjs-rules/property_getUserMedia": 0,
			"scanjs-rules/property_indexedDB": 0,
			"scanjs-rules/property_lastKnownHomeNetwork": 0,
			"scanjs-rules/property_lastKnownNetwork": 0,
			"scanjs-rules/property_localStorage": 2,
			"scanjs-rules/property_mgmt": 0,
			"scanjs-rules/property_sessionStorage": 2,

			"security/detect-non-literal-fs-filename": 0,
			"security/detect-object-injection": 0,
			"no-unsafe-innerhtml/no-unsafe-innerhtml": 0,

			"no-storage/no-browser-storage": 2,
			"no-document-cookie/no-document-cookie": 2
		}
	}
}